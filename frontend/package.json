{
  "name": "alego-manus-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:production": "cross-env NODE_OPTIONS='--max_old_space_size=1024' vite build --mode production",
    "build:check": "tsc --noEmit && vite build --mode production",
    "start": "vite preview --host 0.0.0.0 --port 8080",
    "preview": "vite preview --host 0.0.0.0 --port 8080",
    "test": "vitest",
    "type-check": "tsc --noEmit",
    "heroku-prebuild": "npm install --production=false",
    "prestart": "npm install --only=production --no-audit"
  },
  "engines": {
    "node": "18.20.4",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@react-pdf-viewer/core": "^3.12.0",
    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.0",
    "lucide-react": "^0.525.0",
    "pdfjs-dist": "^3.11.174",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^7.6.3",
    "remark-gfm": "^3.0.1",
    "rollup": "^4.45.1",
    "@rollup/rollup-linux-x64-gnu": "^4.45.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^5.4.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"
  }
}
